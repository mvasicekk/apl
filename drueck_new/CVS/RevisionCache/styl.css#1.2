#scroll
{
	left:0;
	top:0;
	margin: 0 ;
	visibility: visible;
	background-color: white;
	z-index: 1;
	height: 0px;
	overflow: auto
}

#scroll_apl
{
	
	left:0;
	top:0;
	margin: 0 ;
	visibility: visible;
	background-color: rgb(179,179,179);
	z-index: 1;
	height: 300px;
	border-top-style: solid;
	border-right-style: solid;
	border-left-style: solid;
	border-collapse: collapse;
	border-bottom-style: solid;
	border-color: #000000;
	border-width: 1px;
	overflow: auto
}

