/* CSS Document */
table#infotable{
    border-collapse: collapse;
    border: 2px solid greenyellow;
    width: 100%;
    height: 100%;
}

#t1{
    color: white;
    font-family: sans-serif;
    font-size: 50px;
    text-align: center;
    font-weight: normal;
    vertical-align: middle;
}
#t2{
    color: greenyellow;
    font-family: sans-serif;
    font-size: 330px;
    text-align: center;
    font-weight: normal;
}
#t3{
    color: orange;
    font-family: sans-serif;
    font-size: 100px;
    text-align: center;
    font-weight: bold;
}