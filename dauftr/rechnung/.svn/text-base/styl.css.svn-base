}


#scroll
{
	left:0;
	top:0;
	height:300px;
	margin: 0 ;
	visibility: visible;
	background-color: white;
	z-index: 1;
	border-top-style: solid;
	border-right-style: solid;
	border-left-style: solid;
	border-collapse: collapse;
	border-bottom-style: solid;
	border-color: #000000;
	border-width: 1px;
	overflow: auto
}


#scroll div
{
	margin: 0 auto;
	z-index: 1;
	text-align:left
}


