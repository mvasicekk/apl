		}

#umrechformular
{
	border: 1px solid black;
	background-color:rgb(220,220,255);
	height:200px;	
}

#umrechformulardetail
{
	padding:10px;
	width:70%;
}
	
#rechnungstable
{
	height:400px;
	border:1px solid black;
	overflow:auto;
}
	
#scroll_apl
{
	
	left:0;
	top:0;
	margin: 0 ;
	visibility: visible;
	background-color: rgb(179,179,179);
	z-index: 1;
	border-top-style: solid;
	border-right-style: solid;
	border-left-style: solid;
	border-collapse: collapse;
	border-bottom-style: solid;
	border-color: #000000;
	border-width: 1px;
	overflow: auto
}


