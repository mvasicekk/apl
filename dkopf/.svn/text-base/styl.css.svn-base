	font-weight:bold;
}

#content
{
}

#scroll
{
	
	left:0;
	top:0;
	margin: 0 ;
	visibility: visible;
	background-color: white;
	z-index: 1;
	height: 0px;
	border-top-style: solid;
	border-right-style: solid;
	border-left-style: solid;
	border-collapse: collapse;
	border-bottom-style: solid;
	border-color: #000000;
	border-width: 1px;
	overflow: auto
}

#scroll_apl
{
	
	left:0;
	top:0;
	margin: 0 ;
	visibility: visible;
	background-color: rgb(179,179,179);
	z-index: 1;
	height: 300px;
	border-top-style: none;
	border-right-style: none;
	border-left-style: none;
	border-collapse: collapse;
	border-bottom-style: none;
	border-color: #000000;
	border-width: 0px;
	overflow: auto
}

div#dposnew
{
	position: absolute;
	left: 90px;
	bottom: 80px;
	background-color:rgb(240,240,240);
	border: solid 2px red;
	padding:10px;
	margin-left: 5px;
	font-size: 7pt;
	z-index:1500;
	visibility:hidden;
}


#apl_table {
			position: absolute;
			width: 800px;
			left: 0px;
			top: 460px;
			background-color:rgb(220,220,220);
			border: solid 1px black;
			margin-left: 5px;
			font-size: 7pt;
		}

#scroll_attach
{
	
	left:0;
	top:0;
	margin: 0 ;
	visibility: visible;
	background-color: rgb(179,179,179);
	z-index: 1;
	height: 100px;
	border-top-style: solid;
