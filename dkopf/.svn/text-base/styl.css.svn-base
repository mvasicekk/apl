	border-width: 1px;
	overflow: auto
}

#scroll div
{
	margin: 0 auto;
	z-index: 1;
	text-align:left
}


#suggest table
{
	width: 100%;
	font-size: 11px;
	font-weight: normal;
	color: #676767;
	text-decoration: none;
	border: 1px;
	padding: 0px;
	text-align:left;
